<!-- The application sitemesh decorator specifying the name of the jsp that decorates the jsp outputs.
	Excludes decorating jsp pages from the specified list of urls -->
<decorators defaultdir="/WEB-INF/jsp/base">
	<!-- IMPORTANT NOTE: Keep the 'excludes' outside <decorator> tags -->
	<excludes>
		<pattern>*/TestLogins.jsp</pattern>
		<pattern>*logout.do</pattern>
		<pattern>*estimatePrint.do*</pattern>
	</excludes>
	<decorator name="basetemplate" page="sitemesh_base_template.jsp">
		<pattern>/*</pattern>

	</decorator>
</decorators>
